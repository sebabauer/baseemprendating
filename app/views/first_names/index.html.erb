<p id="notice"><%= notice %></p>

<h1>First Names</h1>

<table>
  <thead>
    <tr>
      <th>Last name</th>
      <th>Maiden name</th>
      <th>Formatted name</th>
      <th>Phonetic first name</th>
      <th>Phonetic last name</th>
      <th>Formatted phonetic name</th>
      <th>Headline</th>
      <th>Location</th>
      <th>Industry</th>
      <th>Current share</th>
      <th>Num connections</th>
      <th>Num connections capped</th>
      <th>Sumary</th>
      <th>Specialties</th>
      <th>Positions</th>
      <th>Picture url</th>
      <th>Picture url original</th>
      <th>Site standard profile request</th>
      <th>Api standard profile request</th>
      <th>Public profile url</th>
      <th>Email address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @first_names.each do |first_name| %>
      <tr>
        <td><%= first_name.last_name %></td>
        <td><%= first_name.maiden_name %></td>
        <td><%= first_name.formatted_name %></td>
        <td><%= first_name.phonetic_first_name %></td>
        <td><%= first_name.phonetic_last_name %></td>
        <td><%= first_name.formatted_phonetic_name %></td>
        <td><%= first_name.headline %></td>
        <td><%= first_name.location %></td>
        <td><%= first_name.industry %></td>
        <td><%= first_name.current_share %></td>
        <td><%= first_name.num_connections %></td>
        <td><%= first_name.num_connections_capped %></td>
        <td><%= first_name.sumary %></td>
        <td><%= first_name.specialties %></td>
        <td><%= first_name.positions %></td>
        <td><%= first_name.picture_url %></td>
        <td><%= first_name.picture_url_original %></td>
        <td><%= first_name.site_standard_profile_request %></td>
        <td><%= first_name.api_standard_profile_request %></td>
        <td><%= first_name.public_profile_url %></td>
        <td><%= first_name.email_address %></td>
        <td><%= link_to 'Show', first_name %></td>
        <td><%= link_to 'Edit', edit_first_name_path(first_name) %></td>
        <td><%= link_to 'Destroy', first_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New First Name', new_first_name_path %>
